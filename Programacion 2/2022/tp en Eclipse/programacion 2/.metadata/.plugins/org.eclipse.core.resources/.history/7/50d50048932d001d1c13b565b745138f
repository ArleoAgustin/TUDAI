package tp4.ej5b;

import java.util.ArrayList;

public class Empresa {
	
	private ArrayList<Persona> asociados = new ArrayList<Persona>();

	public Empresa() {

	}

	public ArrayList<Persona> getEmpleados() {
		return empleados;
	}

	public void addEmpleado(Persona e) {
		this.empleados.add(e);
	}
	
	
}
