package TPE2022;

public class Nodo {
	
	private Object contenido;
	private Nodo alQueApunta;
	
	public Nodo(Object contenido) {
		
		this.contenido = contenido;
		this.alQueApunta = null;
	}

	public <E> getContenido() {
		return contenido;
	}

	public void setContenido(Object contenido) {
		this.contenido = contenido;
	}

	public Nodo getAlQueApunta() {
		return alQueApunta;
	}

	public void setAlQueApunta(Nodo alQueApunta) {
		this.alQueApunta = alQueApunta;
	}
	
	
}