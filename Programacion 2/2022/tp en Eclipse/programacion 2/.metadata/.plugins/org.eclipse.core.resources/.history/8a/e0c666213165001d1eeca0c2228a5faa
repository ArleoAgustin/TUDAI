package TPE2022.Comparadores;

import java.util.Comparator;

import TPE2022.ej2.Alumno;

public class ComparadorCompuestoAlumno implements Comparator<Alumno> {
	
	private Comparator<Alumno> c1, c2;
	
	
	
	public ComparadorCompuestoAlumno(Comparator<Alumno> c1, Comparator<Alumno> c2) {
		super();
		this.c1 = c1;
		this.c2 = c2;
	}



	@Override
	public int compare(Alumno o1, Alumno o2) {
    	
		int aux = c1.compare(o1,o2);
        
    	if (aux == 0)
            return c2.compare(o1,o2);
        else return aux;
	}

}
