package EJ4;

public class Cancha {
	
	private String tipo;
	private int valor;
	
	public Cancha(String tipo, int valor) {
		super();
		this.tipo = tipo;
		this.valor = valor;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public int getValor() {
		return valor;
	}

	public void setValor(int valor) {
		this.valor = valor;
	}
		
	
	
}
